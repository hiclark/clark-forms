{
  "name": "@clark-inc/clark-forms",
  "version": "0.6.0",
  "description": "Clark React Redux Forms library.",
  "main": "build/index.js",
  "author": "Clark, Inc.",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiclark/clark-forms.git"
  },
  "keywords": [
    "react",
    "redux",
    "redux-forms",
    "forms",
    "clark"
  ],
  "bugs": {
    "url": "https://github.com/hiclark/clark-forms/issues"
  },
  "homepage": "https://github.com/hiclark/clark-forms#readme",
  "engines": {
    "yarn": ">=1.3.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/index.*",
    "start": "node scripts/start.js & yarn run prettify:watch",
    "lint": "eslint --ext .jsx --ext .js src/ --ignore-pattern node_modules/",
    "lint:fix": "yarn run lint --fix",
    "prettify": "prettier --write '{src,__{tests,mocks}__}/**/*.{js,jsx}'",
    "prettify:watch": "onchange '{src,__{tests,mocks}__}/**/*.{js,jsx}' -- prettier --write {{changed}}",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "yarn test --coverage",
    "build": "node scripts/build.js"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "body-parser": "^1.18.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.28.10",
    "dotenv": "4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "1.1.5",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.3.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "moment": "^2.20.1",
    "object-assign": "4.1.1",
    "onchange": "^3.2.1",
    "prettier": "^1.8.2",
    "promise": "8.0.1",
    "querystring": "^0.2.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-datepicker": "^1.2.1",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.1.1",
    "react-form": "^2.12.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-styled-select": "^2.1.2-alpha",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.1.1",
    "redux": "^3.0.0",
    "redux-form": "^7.2.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "sinon": "^4.3.0",
    "source-map-explorer": "^1.5.0",
    "style-loader": "^0.20.2",
    "styled-components": "2.2.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "plugins": [
      "babel-plugin-styled-components"
    ],
    "presets": [
      "flow",
      "react-app"
    ]
  }
}
